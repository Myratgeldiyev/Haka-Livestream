{
  "expo": {
    "name": "Haka Live",
    "slug": "haka-live",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/new-haka-logo.jpg",
    "scheme": "hakalive",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/Splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#FFFFFF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hakalive.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSMicrophoneUsageDescription": "This app uses the microphone for voice chat in live rooms."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/new-haka-logo.jpg",
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.hakalive.app",
      "permissions": [
        "RECORD_AUDIO",
        "CAMERA",
        "MODIFY_AUDIO_SETTINGS",
        "INTERNET"
      ]
    },
    "web": {
      "output": "static",
      "favicon": ""
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "@react-native-google-signin/google-signin",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#FFFFFF",
          "image": "./assets/images/Splash.png",
          "imageWidth": 280
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "API_URL": "https://wiselabyrinth.pythonanywhere.com/api",
      "eas": {
        "projectId": "f0e2adf4-870c-448c-85d3-fd362201a780"
      }
    },
    "owner": "yunusmyrat"
  }
}
